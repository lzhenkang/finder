appointments/all.html.erb
<div><%= link_to 'New Appointments', "/appointments/new" %></div>
<%= link_to 'Back', root_path %>

<table class="table">
  <thead>
    <tr>
    	<th scope="col">appointment_id</th>
      <th scope="col">user_id</th>
      <th scope="col">time_slot</th>
      <th scope="col">date</th>
      <th scope="col">accepted</th>
      <th scope="col">view appointment</th>
    </tr>
  </thead>
  <tbody>
<% @appointments.each do |appointment| %>
    <tr>
      <td><%= appointment.id %></td>
      <td><%= appointment.user_id %></td>
      <td><%= appointment.time_slot %></td>
      <td><%= appointment.date %></td>
      <td><%= appointment.accepted %></td>
      <td><%= link_to 'View', "/appointments/" + appointment.id.to_s %></td>
    </tr>
<%end%>
  </tbody>
</table>